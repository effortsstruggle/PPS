project(PlugB):acelib{
  sharedname=PlugB
  dynamicflags += TEST_TCP_BUILD_DLL
  libout = ./
  
  Source_Files{
  	./*.cpp 
  }
  specific (gnuace){
		macros += __LINUX__   
		compile_flags += -Wno-deprecated
	}
	
  specific(make) {
    compile_flags += -fcheck-new
  }
  
  specific (prop:microsoft){
	dllout = ../../../../Build/Windows
  } else {
	dllout = ../../../../Build/Linux
  }
  includes += .
  includes += ../../../Common
}
