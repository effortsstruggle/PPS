project(TcpTest):acelib{
  sharedname=TcpTest
  dynamicflags += TEST_TCP_BUILD_DLL
  
  Source_Files{
  	./*.cpp 
  }
  specific (gnuace){
		macros += __LINUX__   
		compile_flags += -Wno-deprecated
		compile_flags += -std=c++11
	}
  
  libout = ./
  specific (prop:microsoft){
	dllout = ../../../Build/Windows
  } else {
	dllout = ../../../Build/Linux
  }
  includes += .
  includes += ../../Common ./IniRead
}
